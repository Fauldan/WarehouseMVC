@model WarehouseMVC_DAL.Models.Fournisseur
@{
    ViewBag.Title = "Details";
}

<h2>Détail du fournisseur</h2>

<p>
    @Html.ActionLink("Créer un fournisseur", "Create")
</p>
<div class="jumbotron">
    <h3>@Html.DisplayFor(model => model.Nom)</h3>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nom
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Nom)
        </dd>
        <dt>
            Numéro TVA
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NumTva)
        </dd>
        <dt>
            Email
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt>
            Téléphone
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NumTel)
        </dd>
        <dt>
            Rue et numéro
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Numero) @Html.DisplayFor(model => model.Rue)
        </dd>
        <dt>
            Code postal
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CodePostal)
        </dd>
        <dt>
            Ville
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Ville)
        </dd>
        <dt>
            Pays
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Pays)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Retour à la liste", "Index")
</p>

